    "@types/winston": "2.4.4",
    "codelyzer": "6.0.2",
    "core-js": "2.6.12",
    "@types/dompurify": "2.3.4",
    "express": "4.17.2",
    "hammerjs": "2.0.8",
    "jasmine": "4.0.2",
    "jasmine-core": "4.0.0",
    "jasmine-marbles": "0.8.4",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "27.4.7",
    "jest-preset-angular": "11.0.1",
